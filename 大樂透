user_a = input('請輸入1~49任意六個數字:')



if int(user_a) < 1:
    print(user_a + ' < 1')
elif int(user_a) > 49:
    print(user_a + ' > 49 ')
else
    a = string(a)
    aa = string(aa)
    special = string(special)
